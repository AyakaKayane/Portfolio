<div class="main users-index">
  <div class="container">
    <h2 class="heading10" data-en="USERS"><span>登録ユーザー一覧</span></h2>
    <div class="userscontainer">
      <% @users.each do |user| %>
        <div class="users-index-item">
          <div class="user-right">
            <%= link_to(user.name, "/users/#{user.id}") %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>